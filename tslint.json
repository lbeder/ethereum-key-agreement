{
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "import-name": false,
    "member-ordering": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "semicolon": "always"
  },
  "extends": [
    "tslint-react",
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ]
}
